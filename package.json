{\n  "name": "real-time-code-collaboration",\n  "version": "1.0.0",\n  "description": "A comprehensive real-time code collaboration platform with multi-user editing, chat, and code execution",\n  "main": "server/index.js",\n  "scripts": {\n    "start": "node server/index.js",\n    "dev": "concurrently \"npm run server\" \"npm run client\"",\n    "server": "nodemon server/index.js",\n    "client": "cd client && npm start",\n    "build": "cd client && npm run build",\n    "install-client": "cd client && npm install",\n    "install-server": "npm install",\n    "install-all": "npm install && npm run install-client"\n  },\n  "dependencies": {\n    "express": "^4.18.2",\n    "socket.io": "^4.7.2",\n    "mongoose": "^7.5.0",\n    "jsonwebtoken": "^9.0.2",\n    "bcryptjs": "^2.4.3",\n    "passport": "^0.6.0",\n    "passport-google-oauth20": "^2.0.0",\n    "passport-github2": "^0.1.12",\n    "passport-jwt": "^4.0.1",\n    "axios": "^1.5.0",\n    "cors": "^2.8.5",\n    "dotenv": "^16.3.1",\n    "express-rate-limit": "^6.8.1",\n    "helmet": "^7.0.0",\n    "morgan": "^1.10.0",\n    "multer": "^1.4.5-lts.1",\n    "validator": "^13.11.0",\n    "compression": "^1.7.4"\n  },\n  "devDependencies": {\n    "nodemon": "^3.0.1",\n    "concurrently": "^8.2.0"\n  },\n  "keywords": ["real-time", "collaboration", "code-editor", "socket.io", "react"],\n  "author": "Aditya Malik",\n  "license": "MIT"\n}